{
	"name": "@gravelly/frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt generate",
		"start": "nuxt start"
	},
	"dependencies": {
		"@mapbox/polyline": "^1.1.1",
		"@nuxtjs/axios": "^5.13.1",
		"dotenv": "^6.2.0",
		"exif-js": "^2.3.0",
		"gpxparser": "^3.0.7",
		"moment": "^2.29.1",
		"nuxt": "^2.15.5",
		"nuxt-client-init-module": "^0.3.0",
		"nuxt-leaflet": "^0.0.25",
		"togpx": "^0.5.4"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@nuxt/babel-preset-app-edge": "^2.16.0-26929477.f08f4efc",
		"@nuxt/types": "^2.15.3",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxt/typescript-runtime": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"core-js": "^3.9.1",
		"cross-env": "^7.0.3",
		"eslint": "^7.24.0",
		"eslint-config-marine": "^8.3.2",
		"eslint-import-resolver-nuxt": "^1.0.1",
		"eslint-plugin-vue": "^7.9.0",
		"node-sass": "^5.0.0",
		"nuxt-typed-vuex": "^0.1.22",
		"typescript": "^4.2.4",
		"vue": "^2.6.12"
	},
	"eslintConfig": {
		"extends": [
			"marine/vue",
			"marine/node"
		],
		"rules": {
			"quote-props": [
				"error",
				"consistent"
			],
			"vue/comma-dangle": [
				"error",
				"never"
			],
			"@typescript-eslint/semi": [
				"error",
				"never"
			],
			"semi": [
				"error",
				"never"
			],
			"comma-dangle": [
				"error",
				"never"
			],
			"@typescript-eslint/space-before-function-paren": [
				"error",
				"always"
			],
			"space-before-function-paren": [
				"error",
				"always"
			],
			"vue/max-attributes-per-line": [
				"error",
				{
					"singleline": {
						"max": 20,
						"allowFirstLine": true
					}
				}
			]
		},
		"settings": {
			"import/resolver": {
				"nuxt": {
					"nuxtSrcDir": "./src",
					"extensions": [
						".js",
						".vue",
						".ts"
					]
				}
			}
		}
	},
	"volta": {
		"node": "16.1.0"
	}
}
